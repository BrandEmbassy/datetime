{
    "name": "brandembassy/datetime",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BrandEmbassy\\DateTime\\": "src"
        }
    },
    "suggest": {
        "doctrine/orm": "Needed for DateTimeImmutableAsTimestampDoctrineType."
    },
    "require": {
        "php": ">=7.4",
        "brandembassy/datetime-factory": "dev-php-8-0 as 1.2.0",
        "nette/utils": "^3.0"
    },
    "require-dev": {
        "brandembassy/coding-standard": "^8.1",
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^8",
        "roave/security-advisories": "dev-master"
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=BrandEmbassyCodingStandard src",
        "phpcbf": "./vendor/bin/phpcbf --standard=BrandEmbassyCodingStandard src",
        "phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon src",
        "phpunit": "./vendor/bin/phpunit src --no-coverage",
        "phpunit-cc": "./vendor/bin/phpunit src",
        "test": "./vendor/bin/phpunit src"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
