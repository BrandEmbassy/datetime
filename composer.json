{
    "name": "brandembassy/datetime",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BrandEmbassy\\DateTime\\": "src"
        }
    },
    "require": {
        "php": "~7.2",
        "beberlei/assert": "^3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-beberlei-assert": "^0.10.0",
        "phpunit/phpunit": "^6.0",
        "roave/security-advisories": "dev-master"
    },
    "scripts": {
        "phpstan": "./vendor/bin/phpstan analyse -l max -c phpstan.neon src",
        "test": "./vendor/bin/phpunit src"
    },
    "config": {
        "sort-packages": true
    }
}
